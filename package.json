{
  "name": "igniteui-angular2",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "npm run typings && npm prune",
    "start": "npm run lite",
    "lite": "lite-server",
    "pretest": "npm run build",
    "build": "node node_modules/typescript/bin/tsc",
    "watch": "node node_modules/typescript/bin/tsc -w",
    "typings": "node node_modules/typings/dist/bin.js install",
    "test": "karma start tests/karma.conf.js"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IgniteUI/igniteui-angular2.git"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.14",
    "systemjs": "0.19.25",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.6.6"
  },
  "devDependencies": {
    "gulp-watch": "^4.3.5",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.2",
    "karma-jasmine": "^0.2.2",
    "karma-junit-reporter": "^0.4.2",
    "lite-server": "^2.2.0",
    "typescript": "^1.8.9",
    "typings": "^0.7.12"
  }
}
